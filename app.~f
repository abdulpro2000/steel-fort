      program App_Steel
              implicit none
              real x, y, z
              integer i
              open(17, file="Steel Sections.csv", status='old')
              read(17, '()')

              do 48 i=1,1
              read(17, *)x,y,z
              write(*,*)x,y,z
48            continue
              close(17)

      end program App_Steel
